{
  "manifest_version": 3,
  "name": "Grow in Juejin",
  "short_name": "Grow",
  "version": "0.1.0",
  "description": "见证在掘金的每一步成长",
  "background": {
    "service_worker": "src/background/index.ts",
    "type": "module"
  },
  "icons": {
    "16": "src/logo/icon16.png",
    "32": "src/logo/icon32.png",
    "48": "src/logo/icon48.png",
    "128": "src/logo/icon128.png"
  },
  "action": {
    "default_title": "打开 Grow in Juejin"
  },
  "content_scripts": [
    {
      "matches": [
        "https://juejin.cn/**"
      ],
      "all_frames": true,
      "js": [
        "src/content-scripts/home/index.ts"
      ]
    }
  ],
  "permissions": [
    "unlimitedStorage",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "author": "water.curly@outlook.com",
  "update_url": "https://gitee.com/curlly-brackets/grow-in-juejin-web-extension/raw/update.xml",
  "key": "",
  "web_accessible_resources": [
    {
      "resources": ["src/app/index.html"],
      "matches": ["https://juejin.cn/*"]
    }
  ]
}